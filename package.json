{
    "name": "LyricService",
    "version": "0.0.1",
    "main": "index.js",
    "scripts": {
        "watch": "jest --testPathIgnorePatterns=\\.aws-sam --detectOpenHandles --watch",
        "test": "jest --testPathIgnorePatterns=\\.aws-sam --detectOpenHandles --forceExit",
        "invoke": "sam local invoke \"LyricService\" -d 5858 -e ./event.json",
        "build": "sam build",
        "debug": "sam local start-api --debug-port 5858 -t ./.aws-sam/build/template.yaml --debug --docker-network lyricservice_default",
        "start": "sam local start-api -t ./.aws-sam/build/template.yaml --docker-network lyricservice_default"
    },
    "devDependencies": {
        "jest": "^26.6.3"
    },
    "dependencies": {
        "auth0-js": "^9.14.0",
        "bufferutil": "^4.0.2",
        "canvas": "^2.6.1",
        "pg": "^8.4.2",
        "sequelize": "^6.3.5",
        "utf-8-validate": "^5.0.3",
        "uuid": "^8.3.1"
    }
}
